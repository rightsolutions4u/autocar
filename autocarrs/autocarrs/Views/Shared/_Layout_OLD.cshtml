                            <div class="col-md-12">
                                <form action="#" method="get">
                                    <div class="form-group">
                                        <input type="email" class="form-control" placeholder="Your Email address*">
                                    </div>
                                    <div class="form-group">
                                        <input type="submit" value="Reset My Password" class="btn btn-block">
                                    </div>
                                </form>
